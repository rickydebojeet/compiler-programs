/* lex program for conversion of infix to postfix */
%{
	#include <stdio.h>
    #include <stdlib.h>
	/* Generated by yacc */
	#include "y.tab.h"
%}

/* Lex definations */
%%
exit.*          {return exit_command;}
quit.*          {return exit_command;}
[0-9]+			{yylval.number = atoi(yytext); return num;}
[+*-/]        	{return yytext[0];}
\n              {return newline;}
[ \t]		    ;
.			    {ECHO; yyerror("Unexpected Character");}
%%

int yywrap() {
	return 1;
}