/* lex program for recognition of string */
%{
	/* Generated by yacc */
	#include "y.tab.h"
%}

/* lex definitions */
%%
exit.*          	{return exit_command;}
quit.*          	{return exit_command;}
[ba]				{return yytext[0];}
\n              	{return newline;}
[ \t]		    	;
.			    	{ECHO; yyerror("Unexpected Character");}
%%

int yywrap() {
	return 1;
}