/* lex program for recognition of palindrome */
%{
	#include <stdio.h>
    #include <stdlib.h>
    #include <string.h>
    /* Generated by yacc */
	#include "y.tab.h"
%}

/* Lex definitions */
%%
exit.*          {return exit_command;}
quit.*          {return exit_command;}
[a-zA-Z]+		{yylval.string = yytext; return STR;}
\n              {return newline;}
[ \t]		    ;
.			    {ECHO; yyerror("Unexpected Character");}
%%

int yywrap() {
	return 1;
}